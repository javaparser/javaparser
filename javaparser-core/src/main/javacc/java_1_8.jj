BlockStmt Block():
{
	Position begin;
	List stmts;
}
{
  { begin = begin(); }
  "{" stmts = Statements() "}"
  { return new BlockStmt(begin, end(), stmts); }
}

/*
 * Classes inside block stametents can only be abstract or final. The semantic must check it.
 */
Statement BlockStatement():
{
	Statement ret;
	Expression expr;
	ClassOrInterfaceDeclaration typeDecl;
	ModifiersAndAnnotations modifier;
}
{
  (
	  LOOKAHEAD( Modifiers() ("class" | "interface") )
  	  {  }
	  modifier = Modifiers()
	  typeDecl = ClassOrInterfaceDeclaration(modifier) { ret = new TypeDeclarationStmt(typeDecl.getBegin(), end(), typeDecl); }
	|
  	  LOOKAHEAD(VariableDeclarationExpression() )
	  expr = VariableDeclarationExpression() ";"
	  { ret = new ExpressionStmt(expr.getBegin(), end(), expr); }
    |
	  ret = Statement()
  )
  { return ret; }
}

VariableDeclarationExpr VariableDeclarationExpression():
{
	ModifiersAndAnnotations modifier;
	Type type;
	List vars = new LinkedList();
	VariableDeclarator var;
}
{
  modifier = Modifiers() type = Type() var = VariableDeclarator() { vars.add(var); } ( "," var = VariableDeclarator() { vars.add(var); } )*
  {
    return new VariableDeclarationExpr(modifier.begin, end(), modifier.modifiers(), modifier.annotations(), type, vars);
  }
}

EmptyStmt EmptyStatement():
{}
{
  ";"
  { return new EmptyStmt(lastBegin(), end()); }
}

Statement LambdaBody():
{
  Expression expr;
  Statement n = null;
}
{
  (
  	expr = Expression()
  	{
  	  n = new ExpressionStmt(expr.getBegin(), end(), expr);
  	}
 	|  n = Block()
  )
  {
  	return n;
  }

}

ExpressionStmt StatementExpression():
/*
 * The last expansion of this production accepts more than the legal
 * Java expansions for StatementExpression.  This expansion does not
 * use PostfixExpression for performance reasons.
 */
{
	Expression expr;
	AssignExpr.Operator op;
	Expression value;
	Type type;
	List typeArgs = null;
	Statement lambdaBody = null;
	VariableDeclaratorId id = null;
	List params = null;
	Expression inner = null;
}
{
  ( LOOKAHEAD(2)
	  expr = PreIncrementExpression()
	|
	  expr = PreDecrementExpression()
	|
	  expr = PrimaryExpression()
	  [
	    "++" { expr = new UnaryExpr(expr.getBegin(), end(), expr, UnaryExpr.Operator.posIncrement);  }
	  |
	    "--" { expr = new UnaryExpr(expr.getBegin(), end(), expr, UnaryExpr.Operator.posDecrement);  }
	  |
	    op = AssignmentOperator() value = Expression() { expr = new AssignExpr(expr.getBegin(), end(), expr, value, op); }
	  | "::"  [typeArgs = TypeParameters() ] (<IDENTIFIER > | "new"){expr = new MethodReferenceExpr(expr.getBegin(), end(), expr, typeArgs, token.image); 	  }

	 |
	   "->" lambdaBody = LambdaBody()
	   {
	     if (expr instanceof CastExpr)
	     {
           inner = generateLambda(((CastExpr)expr).getExpr(), lambdaBody);
           ((CastExpr)expr).setExpr(inner);
	     }
	     else
	     {
	       expr = generateLambda(expr, lambdaBody);
	     }
	   }
	  ]
  )
  ";"
  { return new ExpressionStmt(expr.getBegin(), end(), expr); }
}

