}

SwitchEntryStmt SwitchEntry():
{
	Position begin;
	Expression label = null;
	List stmts;
}
{
  (
	  { begin = begin(); }
	  "case" label = Expression()
	|
	  { begin = begin(); }
	  "default"
  )
  ":" stmts = Statements()
  { return new SwitchEntryStmt(begin, end(), label, stmts); }
}

IfStmt IfStatement():
/*
 * The disambiguating algorithm of JavaCC automatically binds dangling
 * else's to the innermost if statement.  The LOOKAHEAD specification
 * is to tell JavaCC that we know what we are doing.
 */
{
	Position begin;
	Expression condition;
	Statement thenStmt;
	Statement elseStmt = null;
    Comment thenCmmt = null;
    Comment elseCmmt = null;
}
{
  { begin = begin(); }
  "if" "(" condition = Expression() ")" {} thenStmt = Statement() [ LOOKAHEAD(1) "else" {} elseStmt = Statement() ]
  { 
      IfStmt tmp = new IfStmt(begin, end(), condition, thenStmt, elseStmt);
      
      thenStmt.setComment(thenCmmt);
      if (elseStmt != null)
          elseStmt.setComment(elseCmmt);
      return tmp;
  }
}

